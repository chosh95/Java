package decorator;

public class SerializationTest {

class Person{	
	String name;
	String job;
	public Person(String name, String job) {
		this.name = name;
		this.job = job;
	}
	public String toString() {
		return this.name + " " + this.job;
	}
}
	
	
	public static void main(String[] args) {

	}

}
