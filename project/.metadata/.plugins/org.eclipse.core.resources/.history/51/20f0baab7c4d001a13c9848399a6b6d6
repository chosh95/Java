package thread;

class MyThread extends Thread{
	public void run() {
		for(int i=0;i<=200;i++) {
			System.out.println(i + "\t");
			
			try {
				sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}

public class ThreadTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
